	void shapes(Cell board[][50])
{
	char x = '\0';

	int shape = 0;

	printf("Would you like to add a pre-determined shape? (Y/N)\n");

	scanf(" %c", &x);

	if (x == 'Y' || x == 'y')
	{
		printf("what shape would you like to make?\n");
		printf("1. Rectangle\n");
		printf("2. Circle\n");
		printf("3. Triangle\n");

		scanf("%d", &shape);

		if (shape == 1)
		{
			square(board);

		}



	}




}

		
